    public void printTabs() {
        for (var elem : this.tabs) {
            System.out.print(elem);
        }
    }

    public void printTabsReversed() {
        for (Iterator<E> it = this.tabs.reverseIterator(); it.hasNext(); ) {
            System.out.print(it.next());
        }
    }
}
